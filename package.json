{
	"name": "home-page",
	"version": "2.4.1",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": "./server/index.js"
	},
	"description": "A simple browser home page",
	"author": "justfatlard",
	"license": "ISC",
	"engines": {
		"node": ">=16.0.0"
	},
	"scripts": {
		"prepare": "npm run build",
		"start": "npm run dev",
		"dev": "vite",
		"build": "vite build client",
		"build:preview": "vite preview",
		"server": "node ./server/index.js",
		"server:test": "npm run server -- -d ./test/db.json",
		"lint": "eslint --ext .js .",
		"lint:fix": "eslint --fix --ext .js .",
		"pretty": "prettier .",
		"pretty:fix": "prettier --write .",
		"fix": "npm run lint:fix ; npm run pretty:fix",
		"test": "vitest run",
		"test:dev": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fatlard1993/home-page.git"
	},
	"bugs": {
		"url": "https://github.com/fatlard1993/home-page/issues"
	},
	"homepage": "https://github.com/fatlard1993/home-page#readme",
	"dependencies": {
		"@jspm/core": "github:jspm/core",
		"argi": "github:fatlard1993/argi",
		"augmented-ui": "^2.0.0",
		"express": "^4.18.2",
		"lowdb": "^5.1.0",
		"nanoid": "^4.0.2",
		"process": "^0.11.10",
		"relevancy": "^0.2.0",
		"sortablejs": "1.15.0",
		"source-map-js": "^1.0.2",
		"vanilla-bean-components": "github:fatlard1993/vanilla-bean-components#main"
	},
	"devDependencies": {
		"@testing-library/dom": "^9.3.0",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vitest": "0.2.5",
		"eslint-plugin-vitest-globals": "^1.3.1",
		"jsdom": "^21.1.2",
		"prettier": "2.8.8",
		"vite": "^4.3.9",
		"vitest": "^0.31.1"
	}
}
