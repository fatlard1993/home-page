{
	"name": "home-page",
	"version": "2.4.3",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": "./server/index.js"
	},
	"description": "A simple browser home page",
	"author": "justfatlard",
	"license": "ISC",
	"engines": {
		"node": ">=16.0.0"
	},
	"browserslist": [
		"last 1 version",
		"> 1%",
		"not dead",
		"not op_mini all"
	],
	"scripts": {
		"prepare": "npm run build",
		"start": "npm run dev",
		"dev": "vite",
		"build": "vite build client",
		"build:preview": "vite preview",
		"server": "node ./server/index.js",
		"server:test": "npm run server -- -d ./test/db.json",
		"lint": "eslint --ext .js .",
		"lint:fix": "eslint --fix --ext .js .",
		"pretty": "prettier .",
		"pretty:fix": "prettier --write .",
		"fix": "npm run lint:fix ; npm run pretty:fix",
		"test": "npm run lint && vitest run",
		"test:fix": "npm run fix && vitest run",
		"test:force": "vitest run",
		"test:dev": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fatlard1993/home-page.git"
	},
	"bugs": {
		"url": "https://github.com/fatlard1993/home-page/issues"
	},
	"homepage": "https://github.com/fatlard1993/home-page#readme",
	"dependencies": {
		"@jspm/core": "github:jspm/core",
		"argi": "github:fatlard1993/argi",
		"augmented-ui": "^2.0.0",
		"express": "^4.18.2",
		"lowdb": "^6.0.1",
		"nanoid": "^4.0.2",
		"process": "^0.11.10",
		"relevancy": "^0.2.0",
		"sortablejs": "1.15.0",
		"source-map-js": "^1.0.2",
		"vanilla-bean-components": "github:fatlard1993/vanilla-bean-components#main"
	},
	"devDependencies": {
		"@testing-library/dom": "^9.3.1",
		"@vitejs/plugin-legacy": "^4.1.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-compat": "^4.1.4",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-spellcheck": "0.0.20",
		"eslint-plugin-testing-library": "^5.11.0",
		"eslint-plugin-unicorn": "^47.0.0",
		"eslint-plugin-vitest": "0.2.6",
		"eslint-plugin-vitest-globals": "^1.4.0",
		"eslint-plugin-write-good-comments": "^0.2.0",
		"jsdom": "^22.1.0",
		"prettier": "3.0.0",
		"vite": "^4.4.3",
		"vitest": "^0.33.0"
	}
}
